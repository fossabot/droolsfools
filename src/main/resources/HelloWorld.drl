import hu.lsm.droolsfools.dto.IncomingDataAdapter;

rule "%s"
      dialect "mvel"
  when
    m : IncomingDataAdapter( incomingData.errorCode == 200, message : message )
  then

    System.out.println( message );
    //modify ( m ) { message = "OK" };
    m.setMessage("OK");
    System.out.println( message );
end